{
  "extends": "default",
  "rules": {
    "indent": ["error", 4],
    "quotes": ["error", "double"],
    "max-line-length": ["error", 125],
    "avoid-sha3": "warn",
    "avoid-suicide": "error",
    "avoid-throw": "error",
    "reentrancy": "error",
    "func-visibility": "error",
    "state-visibility": "error",
    "multiple-sends": "error"
  }
}